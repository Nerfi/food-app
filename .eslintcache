 user tags\n      const selectedTags = tags.join();\n      const url = `https://api.spoonacular.com/recipes/random?number=6&tags=${selectedTags}&apiKey=${API_SECRET}`;\n      //making the request\n      await apiHelper(url)\n        .then(res => setRandom({res: res.recipes}))\n        .catch(e => setError(e.message))\n  };\n\n\nreturn (\n    <div className='content'>\n      <div>{error && <p>something went wrong...</p>}</div>\n\n      <TagsSelection select={select} tags={tags}/>\n\n     <div className=\"button\">\n      <button onClick={fetchRandom}>Search for random recipies</button>\n     </div>\n\n     <div className=\"displayMeals\">\n      {random?.res?.map(meal => <FoodCard {...meal} key={meal.id}/> )}\n    </div>\n\n  </div>\n)\n\n};\n\nexport default RandomMeal;\n","/home/juanfran/code/Nerfi/food-app/src/components/UI/TagsSelection.js",[],"/home/juanfran/code/Nerfi/food-app/src/AuthContext/UserContext.js",[],"/home/juanfran/code/Nerfi/food-app/src/firebase/firebase.js",[],"/home/juanfran/code/Nerfi/food-app/src/Auth/Login.js",["99"],"import React,{useState, useContext} from 'react';\nimport { firebase} from '../firebase/firebase';\nimport './Login.css';\nimport {useHistory, Link} from 'react-router-dom';\nimport { UserContext} from '../AuthContext/UserContext';\n\n\nfunction Login () {\n\n  const [email, setEmail] = useState('')\n  const [password, setPassword] = useState('');\n  const [error, setError] = useState(null);\n  const {login} = useContext(UserContext);\n  const history = useHistory();\n\n\n\nconst loginUser = async(e) => {\n  e.preventDefault();\n\n  try {\n    await login(email, password)\n    history.push('/');\n\n  }catch(e){\n    setError(e.message);\n  }\n\n}\n\n  return(\n    <div className=\"loginContainer\">\n\n    <form onSubmit={loginUser} className=\"loginForm\">\n    {error && <p>{error}</p>}\n\n      <h2 className=\"loginH2\">Login!</h2>\n      <label>Email</label>\n      <div className=\"input\">\n       <input type=\"email\" name=\